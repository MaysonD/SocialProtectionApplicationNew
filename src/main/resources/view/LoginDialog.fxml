<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="loginPane" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="340.0" prefWidth="250.0"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="com.module.controllers.LoginDialogController">
    <children>
        <Label layoutX="84.0" layoutY="69.0" prefHeight="17.0" prefWidth="82.0" text="Введите логин"
               AnchorPane.leftAnchor="84.0" AnchorPane.rightAnchor="84.0" AnchorPane.topAnchor="70.0"/>
        <Label layoutX="79.0" layoutY="138.0" text="Введите пароль" AnchorPane.leftAnchor="80.0"
               AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="140.0"/>
        <PasswordField fx:id="inputPasswordField" focusTraversable="false" layoutX="49.0" layoutY="170.0"
                       prefHeight="25.0" prefWidth="180.0" promptText="пароль" AnchorPane.leftAnchor="40.0"
                       AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="170.0"/>
        <Label fx:id="errorLoginLabel" layoutX="17.0" layoutY="201.0" prefHeight="46.0" prefWidth="214.0"
               text="Вы ввели неверный логин или пароль. Повторите попытку снова." textFill="RED" visible="false"
               wrapText="true" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0"/>
        <Button fx:id="loginButton" focusTraversable="false" layoutX="98.0" layoutY="260.0" mnemonicParsing="false"
                onAction="#setLoginAction" prefHeight="25.0" prefWidth="75.0" text="Войти" AnchorPane.leftAnchor="95.0"
                AnchorPane.rightAnchor="95.0" AnchorPane.topAnchor="260.0"/>
        <TextField fx:id="inputDistrictField" focusTraversable="false" layoutX="49.0" layoutY="99.0" prefHeight="25.0"
                   prefWidth="180.0" promptText="пользователь" AnchorPane.leftAnchor="40.0"
                   AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="100.0"/>
    </children>
</AnchorPane>
