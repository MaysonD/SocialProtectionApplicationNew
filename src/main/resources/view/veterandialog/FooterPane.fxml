<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="footerPane" prefHeight="30.0" prefWidth="780.0"
            xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="com.module.controllers.veterandialog.FooterPaneController">
    <children>
        <HBox prefHeight="32.0" prefWidth="190.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <children>
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonClick" text="Сохранить"/>
                <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#cancelButtonClick"
                        text="Отмена">
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</AnchorPane>
